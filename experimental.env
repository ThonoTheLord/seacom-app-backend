# Experimental Database Configuration (PostGIS)
DB_HOST=localhost
DB_USER=postgres
DB_PASSWORD=admin123
DB_PORT=5432
DB_NAME=seacom_experimental_db

# Security Configuration
JWT_TOKEN_EXPIRE_MINUTES=30
JWT_REFRESH_TOKEN_EXPIRE_DAYS=7
# Generate with: python -c 'import secrets; print(secrets.token_urlsafe(32))'
JWT_SECRET_KEY=experimental_secret_key_change_in_production
JWT_ALGORITHM=HS256

# CORS Configuration (comma-separated list of allowed origins)
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173

# Presence (optional Redis) - enable Redis presence in experimental environment
# To use Redis set PRESENCE_BACKEND=redis and provide REDIS_URL below
PRESENCE_BACKEND=redis
REDIS_URL=redis://localhost:6379/0
PRESENCE_REDIS_TTL_SECONDS=300
PRESENCE_PUBSUB_CHANNEL=presence_events
